<%= form_for @event do |f| %>
	<%= f.label :name, "Event Name" %><br/>
	<%= f.text_field :name %><br/>

	<%= f.label :date, "Date" %><br/>
	<%= f.date_field :date %><br/>

	<%= f.label :description %><br/>
	<%= f.text_area :description, rows: 3 %><br/>

	<%= f.label :location %><br/>
	<%= f.text_field :location %><br/>

	<%= f.label :picture, "Picture Link" %><br/>
	<%= f.text_field :picture %><br/><br/>

	<%= f.submit "Create Event" %>
<% end %>