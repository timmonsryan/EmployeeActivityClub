<% provide(:heading, "Sign In") %>
<%= form_tag sessions_path do %>
	<%= label_tag :tellerNumber, "Teller Number" %><br>
	<%= text_field_tag :tellerNumber %><br><br>

	<%= label_tag :password %><br>
	<%= password_field_tag :password %><br><br>

	<%= submit_tag "Sign In" %>
<% end %><br>

Not a user? <%= link_to "Sign up now!", signup_path %>